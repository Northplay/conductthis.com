{
  "root": "build/",
  "routes": {
    "/**": "index.html"
  },
  "https_only": true,
  "headers": {
    "/**": {
      "Strict-Transport-Security": "max-age=2592000;",
      "X-Frame-Options": "SAMEORIGIN",
      "X-Xss-Protection": "1; mode=block",
      "X-Content-Type-Options": "nosniff"
    }
  }
}
